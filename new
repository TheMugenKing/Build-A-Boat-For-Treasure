    -- Membuat platform aman di bawah karakter
    local block = Instance.new("Part")
    block.Size = Vector3.new(10, 1, 10)
    block.Anchored = true
    block.CanCollide = true
    block.Position = pos
    block.Transparency = 0.5
    block.BrickColor = BrickColor.new("Bright green")
    block.Name = "SafeBlock_" .. i
    block.Parent = workspace

    -- Menghapus komponen yang mungkin menyebabkan kerusakan
    for _, d in pairs(block:GetDescendants()) do
        if d:IsA("TouchTransmitter") then
            d:Destroy()
        end
    end
end

end)

