-- Fungsi utama loop teleport + buat platform
local function startLoop(char)
    local hrp = char:WaitForChild("HumanoidRootPart")
    coroutine.wrap(function()
        while char and char.Parent do
            for i, pos in ipairs(SpawnList) do
                task.wait(0)

                -- Teleport ke posisi + sedikit di atas block
                hrp.CFrame = CFrame.new(pos + Vector3.new(0, 5, 0))

                -- Buat platform aman
                local block = Instance.new("Part")
                block.Size = Vector3.new(10, 1, 10)
                block.Anchored = true
                block.CanCollide = true
                block.Position = pos
                block.Transparency = 0.5
                block.BrickColor = BrickColor.new("Bright green")
                block.Name = "SafeBlock_" .. i .. "_" .. tostring(math.random(1000,9999))
                block.Parent = workspace
            end
        end
    end)()
end

-- Pertama kali jalan
if player.Character then
    startLoop(player.Character)
end

-- Kalau mati & respawn, tetap jalanin ulang
player.CharacterAdded:Connect(function(char)
    startLoop(char)
end)

end)

