-- Buat GUI hanya jika belum ada
local gui = player:WaitForChild("PlayerGui"):FindFirstChild("TimePlayedGui")
if not gui then
    gui = Instance.new("ScreenGui")
    gui.Name = "TimePlayedGui"
    gui.ResetOnSpawn = false
    gui.Parent = player:WaitForChild("PlayerGui")

    local timeLabel = Instance.new("TextLabel")
    timeLabel.Name = "TimeLabel"
    timeLabel.Size = UDim2.new(0, 200, 0, 30)
    timeLabel.Position = UDim2.new(0, 10, 0, 10)
    timeLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    timeLabel.TextColor3 = Color3.new(1, 1, 1)
    timeLabel.TextSize = 14
    timeLabel.Font = Enum.Font.SourceSansBold
    timeLabel.Text = "Time Played: 00:00"
    timeLabel.Parent = gui
end

-- Ambil label
local timeLabel = gui:WaitForChild("TimeLabel")

-- Hitung waktu bermain
local playTime = 0
RunService.RenderStepped:Connect(function(dt)
    playTime += dt
    local minutes = math.floor(playTime / 60)
    local seconds = math.floor(playTime % 60)
    timeLabel.Text = string.format("Time Played: %02d:%02d", minutes, seconds)
end)

-- Fungsi utama loop teleport + buat platform
local function startLoop(char)
    local hrp = char:WaitForChild("HumanoidRootPart")
    coroutine.wrap(function()
        while char and char.Parent do
            for i, pos in ipairs(SpawnList) do
                task.wait(0)

                -- Teleport ke posisi + sedikit di atas block
                hrp.CFrame = CFrame.new(pos + Vector3.new(0, 5, 0))

                -- Buat platform aman
                local block = Instance.new("Part")
                block.Size = Vector3.new(10, 1, 10)
                block.Anchored = true
                block.CanCollide = true
                block.Position = pos
                block.Transparency = 0.5
                block.BrickColor = BrickColor.new("Bright green")
                block.Name = "SafeBlock_" .. i .. "_" .. tostring(math.random(1000,9999))
                block.Parent = workspace
            end
        end
    end)()
end

-- Pertama kali jalan
if player.Character then
    startLoop(player.Character)
end

-- Kalau mati & respawn, tetap jalanin ulang
player.CharacterAdded:Connect(function(char)
    startLoop(char)
end)

end)

